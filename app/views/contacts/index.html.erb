<p id="notice"><%= notice %></p>

<h1>Contacts</h1>

<table class="table">
  <thead>
    <tr>
      <th colspan="3">Nombre</th>
      <th colspan="3">Fecha De Nacimiento</th>
      <th colspan="3">Telefono</th>
      <th colspan="3">Direccion</th>
      <th colspan="3">Tarjeta de Credito</th>
      <th colspan="3">Franquicia</th>
      <th colspan="3">Email</th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td colspan="3"><%= contact.name %></td>
        <td colspan="3"><%= contact.dob %></td>
        <td colspan="3"><%= contact.telephone %></td>
        <td colspan="3"><%= contact.address %></td>
        <td colspan="3"><%= contact.credit_card %></td>
        <td colspan="3"><%= contact.franchise %></td>
        <td colspan="3"><%= contact.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact', new_contact_path %>
